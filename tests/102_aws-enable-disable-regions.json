{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "EnableDisableHongKong",
          "Effect": "Allow",
          "Action": [
              "account:EnableRegion",
              "account:DisableRegion"
          ],
          "Resource": "*",
          "Condition": {
              "StringEquals": {"account:TargetRegion": "ap-east-1"}
          }
      },
      {
          "Sid": "ViewConsole",
          "Effect": "Allow",
          "Action": [
              "account:ListRegions"
          ],
          "Resource": "*"
      }
  ]
}
